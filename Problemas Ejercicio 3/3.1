#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i;
    int num;
    long factorial = 1;

    printf("Ingrese el numero por favor: ");
    scanf("%d", &num);

    if (num >= 0)
    {
        for (i = 1; i <= num; i++)
        {
            factorial = factorial * i;
        }

        printf("\nEl factorial de %d es: %ld", num, factorial);
    }
    else
    {
        printf("\nError en el dato ingresado");
    }

    return 0;
}
